import java.util.LinkedList;
import java.util.NoSuchElementException;
import java.util.ArrayList;

public class ElectionBackEnd{
  
  HashTableMap map = new HashTableMap();
  USElectionData data = new USElectionData();
  HashTableMap<String, ArrayList<String>> table = data.getDataHashTable();
  ArrayList<String> states = data.getHashTableKeys();
  
  

   /**
   * Returns the result of election in a particular state
   * @param state in which winner is requested
   * @return the winner
   */
  public ArrayList<String> getResult(String state) {
     ArrayList<String> result=null;
    String winner = null; 
    for(int i=0;i<4;i++) {
    winner = (String) map.get(state);
    result.add(winner);
    
    }
    return result;
  }
     
   
  
   /**
   * Gives the hash value of the state
   * @param state which is required
   * @return hashcode of the state
   */
  public int getState(String state) {
     
    int hashCode = state.hashCode();
    
    return hashCode;     
   }
  
  /**
   * Checks if the state is actually present
   * @param state to be checked
   */
  public void getContainsKey(String state) {
    boolean result = map.containsKey(state);
    System.out.println(result);
  }
}
